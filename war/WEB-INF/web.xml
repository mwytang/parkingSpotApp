<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ParkingSpotsApp</servlet-name>
		<servlet-class>ca.appengine.project.parkingspots.ParkingSpotsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ParkingSpotsApp</servlet-name>
		<url-pattern>/parkingspotsapp</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>MakeBookingServlet</servlet-name>
	    <servlet-class>ca.appengine.project.parkingspots.MakeBookingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>MakeBookingServlet</servlet-name>
	    <url-pattern>/booking</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>QueryProcessor</servlet-name>
	    <servlet-class>ca.appengine.project.parkingspots.QueryProcessor</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>QueryProcessor</servlet-name>
	    <url-pattern>/queryprocessor</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
